{
  "name": "project_dashboard",
  "version": "0.0.1",
  "description": "Project dashboard demo",
  "main": "index.html",
  "scripts": {
    "start": "node heroku_server/index.js",
    "test": "jest ./src --env=jsdom",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest ./src --runInBand",
    "test:publish": "JEST_JUNIT_OUTPUT=\"./TEST-RESULTS.xml\" jest --ci --reporters=default --reporters=jest-junit",
    "test:watch": "jest ./src -- watch",
    "test:integration": "NO_SERVER=true jest ./integration --env=jsdom --detectOpenHandles",
    "test:integration:debug": "NO_SERVER=true node --inspect-brk ./node_modules/.bin/jest ./integration --runInBand --detectOpenHandles",
    "test:integration:watch": "NO_SERVER=true jest ./integration --watch --env=jsdom --detectOpenHandles",
    "test:e2e": "cypress open",
    "dev": "concurrently \"cross-env NODE_ENV=dev parcel index.html\" \"yarn api\"",
    "dev:express": "concurrently \"yarn dev\" && \"node heroku_server/index.js\"",
    "prod": "cross-env NODE_ENV=prod parcel build index.html",
    "prettier:run": "prettier --single-quote --write \"src/**/*.{js,jsx,scss,css}\"",
    "prettier:run:tests": "prettier --single-quote --write \"cypress/**/*.js\"",
    "lint:run": "yarn prettier:run && yarn eslint:run",
    "lint:run:tests": "yarn prettier:run:tests && yarn eslint:run:tests",
    "eslint:run": "eslint src/**/*.{js,jsx} --fix --quiet",
    "eslint:run:tests": "eslint cypress/**/*.js --fix --quiet",
    "api": "yarn clear:api && yarn compile:api && yarn run:api && yarn copy:api",
    "run:api": "node $NODE_DEBUG_OPTION dist/server/mockServer.js",
    "clear:api": "rimraf dist/server",
    "compile:api": "babel src/server --out-dir dist/server",
    "copy:api": "cp dist/server/db.json src/server/db.json"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "@blueprintjs/core": "^3.22.3",
    "@blueprintjs/icons": "^3.13.0",
    "@blueprintjs/table": "^3.8.2",
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free": "^5.12.0",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@reach/router": "^1.2.1",
    "@types/reach__router": "^1.2.6",
    "axios": "^0.21.1",
    "concurrently": "^5.0.2",
    "core-js": "^3.6.4",
    "cross-env": "^6.0.3",
    "deep-freeze": "^0.0.1",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "lo": "^0.40.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "nprogress": "^0.2.0",
    "parcel-bundler": "^1.12.4",
    "qs": "^6.9.1",
    "react": "^16",
    "react-calendar": "^2.19.2",
    "react-clamp-lines": "^3.0.2",
    "react-date-picker": "^7.10.0",
    "react-dom": "^16",
    "react-is": "^16.12.0",
    "react-select": "^3.0.8",
    "react-transition-group": "^4.3.0",
    "regenerator-runtime": "^0.13.3",
    "shortid": "^2.2.15",
    "smoothscroll-polyfill": "^0.4.4",
    "src": "^1.1.2",
    "stickybits": "^3.7.3",
    "styled-components": "^5.0.0",
    "yup": "^0.28.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-flow": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@cypress/webpack-preprocessor": "^4.1.1",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-styled-components": "^1.10.6",
    "body-parser": "^1.19.0",
    "cp": "^0.2.0",
    "css-loader": "^3.4.2",
    "cypress": "3.8.2",
    "cypress-jest-adapter": "^0.1.1",
    "cypress-parcel-preprocessor": "^1.0.0",
    "cypress-pipe": "^1.6.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-cypress": "^2.8.1",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "express-list-endpoints": "^4.0.1",
    "fibers": "^4.0.2",
    "file-loader": "^5.0.2",
    "flow-bin": "^0.116.1",
    "flow-typed": "^2.6.2",
    "husky": "^4.0.10",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-haste-map": "^24.9.0",
    "jest-junit": "^10.0.0",
    "jest-mock-axios": "^3.2.0",
    "jest-resolve": "^24.9.0",
    "jest-runner-eslint": "^0.7.5",
    "json-server": "^0.15.1",
    "lint-staged": "^9.5.0",
    "lowdb": "^1.0.0",
    "nodemon": "^2.0.2",
    "npm-check-updates": "^4.0.1",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "rimraf": "^3.0.0",
    "sass": "^1.24.5",
    "sass-lint": "^1.13.1",
    "sass-loader": "^8.0.2",
    "stacktrace-js": "^2.0.2",
    "style-loader": "^1.1.2",
    "superagent": "^5.2.1",
    "supertest": "^4.0.2",
    "webpack": "^4.41.5"
  },
  "proxy": "http://localhost:8080",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{css,scss,jsx,js,mjs,json}": [
      "prettier --single-quote --write",
      "git add"
    ],
    "src/**/*.{mjs,js,jsx}": [
      "eslint --fix --quiet",
      "git add"
    ],
    "cypress/**/*.{mjs,js,jsx}": [
      "prettier --write",
      "eslint --fix --quiet",
      "git add"
    ],
    "integration/**/*.{mjs,js,jsx}": [
      "prettier --write",
      "eslint --fix --quiet",
      "git add"
    ],
    "**/*.md": [
      "prettier --single-quote --write",
      "git add"
    ]
  }
}
